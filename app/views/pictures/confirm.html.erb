<%= form_with(model: @picture, local: true, url: pictures_path) do |form|%>
    <%= form.hidden_field :content%>
    <%= form.hidden_field :image%>
    <%= form.hidden_field :image_cache%>
    <%= form.hidden_field :user_id%>
    <p><%= @picture.content%></p><br>
    <p><%= image_tag(@picture.image.url, class:"images") if @picture.image && @picture.image.url %></p><br>
    <%= form.submit "submit"%>
    <%= form.submit "back", name: 'back' %>
<% end %>